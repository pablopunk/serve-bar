{
  "name": "serve-bar",
  "version": "1.0.0",
  "description": "Share folders in your network using serve",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "standard",
    "dist": "build"
  },
  "build": {
    "appId": "com.pablopunk.serve-bar",
    "files": [
      "**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "asar": false
  },
  "keywords": [
    "server",
    "share",
    "server",
    "zeit",
    "topbar"
  ],
  "author": "Pablo Varela",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^18.6.2",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "node-notifier": "^5.1.2",
    "serve": "^5.2.2"
  }
}
